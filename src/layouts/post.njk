---
layout: page.njk
title: Blog
---


<article class="article">

  <h2 class="article-title">
    {{ title }}
  </h2>

  <time class="article-date" datetime="{{ date }}">{{ date | dateShort }}</time>

  {% include 'tags.njk' %}

  <div class="article-content">
    {{ content | safe }}
  </div>

</article>



